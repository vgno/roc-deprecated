{
  "name": "roc",
  "version": "1.0.0-alpha6",
  "description": "CLI for the Roc ecosystem",
  "main": "./lib/index.js",
  "bin": "./lib/cli/roc.js",
  "preferGlobal": true,
  "scripts": {
    "build": "babel src --out-dir lib --source-maps",
    "build:watch": "npm run build -- --watch",
    "clean": "rimraf lib docs coverage",
    "test": "npm run lint && npm run test:coverage",
    "test:unit": "mocha --compilers js:babel/register test/",
    "test:unit:watch": "mocha --watch --compilers js:babel/register test/",
    "test:coverage": "isparta cover _mocha -- --compilers js:babel/register",
    "lint": "eslint .",
    "check": "ncu",
    "prepublish": "npm test && npm run clean && npm run build"
  },
  "author": "VG",
  "license": "MIT",
  "dependencies": {
    "colors": "~1.1.2",
    "commander": "~2.9.0",
    "fs-extra": "~0.26.2",
    "inquirer": "~0.11.0",
    "lodash": "~3.10.1",
    "replace": "~0.3.0",
    "request": "~2.65.0",
    "roc-config": "*",
    "semver": "~5.0.3",
    "source-map-support": "~0.3.2",
    "tar": "~2.2.1",
    "temp": "~0.8.3"
  },
  "devDependencies": {
    "babel": "~5.8.34",
    "babel-eslint": "~4.1.1",
    "chai": "~3.4.1",
    "codeclimate-test-reporter": "~0.1.1",
    "coveralls": "~2.11.4",
    "eslint": "~1.9.0",
    "eslint-config-vgno": "~5.0.0",
    "isparta": "~3.5.3",
    "mocha": "~2.3.4",
    "npm-check-updates": "~2.4.2",
    "rimraf": "~2.4.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vgno/roc.git"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "roc"
  ]
}
